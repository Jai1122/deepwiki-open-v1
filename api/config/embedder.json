{
  "embedder": {
    "client_class": "OpenAIClient",
    "initialize_kwargs": {
      "api_key": "${VLLM_API_KEY}",
      "base_url": "${VLLM_EMBEDDING_BASE_URL}"
    },
    "batch_size": 10,
    "model_kwargs": {
      "model": "REPLACE_WITH_YOUR_VLLM_EMBEDDING_MODEL_ID",
      "dimensions": 256
    }
  },
  "retriever": {
    "top_k": 20
  },
  "text_splitter": {
    "split_by": "word",
    "chunk_size": 1000,
    "chunk_overlap": 200
  }
}
