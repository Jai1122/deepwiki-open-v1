{
  "embedding_models": {
    "/app/models/jina-embeddings-v3": {
      "api_url": "https://vllm.com/jina-embeddings-v3/v1",
      "dimensions": 1024,
      "display_name": "Jina Embeddings v3"
    },
    "/app/models/text-embedding-ada-002": {
      "api_url": "https://api.openai.com/v1",
      "dimensions": 1536,
      "display_name": "OpenAI Ada 002"
    },
    "/app/models/bge-large-en-v1.5": {
      "api_url": "https://vllm.com/bge-large-en-v1.5/v1",
      "dimensions": 1024,
      "display_name": "BGE Large EN v1.5"
    }
  },
  "default_embedding_model": "/app/models/jina-embeddings-v3",
  "embedder": {
    "client_class": "OpenAIClient",
    "initialize_kwargs": {
      "api_key": "${VLLM_API_KEY}",
      "base_url": "DYNAMIC_FROM_MODEL_CONFIG"
    },
    "batch_size": 10,
    "model_kwargs": {
      "model": "DYNAMIC_FROM_MODEL_CONFIG",
      "dimensions": "DYNAMIC_FROM_MODEL_CONFIG"
    }
  },
  "retriever": {
    "top_k": 20
  },
  "text_splitter": {
    "split_by": "word",
    "chunk_size": 400,
    "chunk_overlap": 50,
    "max_tokens_per_chunk": 4000
  }
}
